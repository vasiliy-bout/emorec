<?xml version="1.0" encoding="utf-8" ?>
<config>
	<property name="cascade" value="haarcascades/haarcascade_frontalface_alt.xml" />
	<property name="mlp" value="mlp/eigen-vk-train_on-dcim.xml" />
	<property name="pca" value="pca/vk.xml" />
	<!--	
        featuresArg parameter:
           featuresArg <= 0.0f       - load all available eigenfaces
           0.0f < featuresArg < 1.0f - load eigenfaces for which (eigenvalue >= featuresArg * max(eigenvalue))
           featuresArg >= 1.0f       - load at most ((int) featuresArg) first eigenfaces
	-->
	<property name="features" value="0.01" />
	<property name="scale" value="1.1" />
	<property name="minFacePercent" value="0.25" />
</classes>
